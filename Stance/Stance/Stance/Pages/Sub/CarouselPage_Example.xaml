<?xml version="1.0" encoding="utf-8" ?>
<CarouselPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Stance.Pages.Sub.CarouselPage_Example"
             Title="Carousel Page">

  <ContentPage>
    <StackLayout>
      <Image Source="kick.jpg" VerticalOptions="FillAndExpand" Aspect="AspectFill"/>
    </StackLayout>
  </ContentPage>
  <ContentPage>
    <StackLayout>
      <Image Source="kettlebellswatt.jpg" Aspect="AspectFill"/>

    </StackLayout>
  </ContentPage>
  <ContentPage>
    <StackLayout>
      <Image Source="cablecurls.jpg" VerticalOptions="CenterAndExpand" Aspect="AspectFill"/>

    </StackLayout>
  </ContentPage>
  

  </CarouselPage>